{
  "version": 2,
 
  "routes": [
    {
      "src": "/db/(.*)",
      "dest": "/db/$1"
    },
    {
      "src": "/models/(.*)",
      "dest": "/models/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/views/(.*)",
      "dest": "/views/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "DATABASE_URL": "/db/database.sqlite",
    "NODE_ENV": "production"
  }
}


