{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/views/index.ejs"
    },
    {
      "src": "/form",
      "dest": "/views/form.ejs"
    }
  ],
  "env": {
    "DATABASE_URL": "/db/database.sqlite",
    "NODE_ENV": "production"
  },
  "includeFiles": [
    "db/**",
    "models/**",
    "public/**",
    "views/**",
    "app.js",
    "db.js",
    "package.json",
    "package-lock.json"
  ]
}

